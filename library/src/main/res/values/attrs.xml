<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTextView">
        <attr name="font" format="reference" />
    </declare-styleable>

    <declare-styleable name="LuckyWheelView">
        <attr name="lkwBackgroundColor" format="color" />
        <attr name="lkwCursor" format="reference" />
        <attr name="lkwCenterImage" format="reference" />
        <attr name="lkwEdgeColor" format="color" />
        <attr name="lkwEdgeWidth" format="dimension" />
        <attr name="lkwTopTextColor" format="color" />
        <attr name="lkwTopTextPadding" format="dimension" />
        <attr name="lkwTopTextSize" format="dimension" />
        <attr name="lkwSecondaryTextSize" format="dimension" />
        <attr name="lkwSecondaryTextPadding" format="dimension" />
        <attr name="lkwSpinDuration" format="integer"/>
        <attr name="lkwDecelarationDuration" format="integer"/>
        <attr name="lkwWheelRotation" format="integer"/>
        <attr name="lkwEnableWheelBlur" format="boolean"/>
        <attr name="lkwWheelCircleDiameter" format="dimension" />
        <attr name="lkwWheelCenterCircleSize" format="dimension" />
        <attr name="lkwShowSliceView" format="boolean"/>
        <attr name="lkwShowBlurView" format="boolean"/>
        <attr name="lkwBlurViewDuration" format="integer"/>
    </declare-styleable>

</resources>